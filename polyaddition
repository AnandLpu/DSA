#include<stdio.h>
int main()
{
    int m,n,i,j;
    printf("Enter the size of array a[]:-\n");
    scanf("%d",&m);
    printf("Enter the size of array b[]:-\n");
    scanf("%d",&n);
    int a[m],b[n];
    printf("Enter the elements of array a[]:-\n");
    for(i=0;i<m;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the elements of array b[]:-\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    if(m>n)
    {
        int j=m;
        int sum[j];
        for(i=0;i<j;i++)
        {
            sum[i]=a[i];
        }
        for(i=0;i<n;i++)
        {
            sum[i]=sum[i]+b[i];
        }
        for(i=j-1;i>=0;i--)
        {
            printf("%d ",sum[i]);
        }
    }
    else{
        
        int j=n;
        int sum[j];
        for(i=0;i<j;i++)
        {
            sum[i]=b[i];
        }
        for(i=0;i<m;i++)
        {
            sum[i]=sum[i]+a[i];
        }
        for(i=j-1;i>=0;i--)
        {
            printf("%d ",sum[i]);
        }
    }return 0;
}
